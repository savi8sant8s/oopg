datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Adm {
  id           Int @id @default(autoincrement())
  creationDate DateTime @default(now())
  updatedDate  DateTime?
  name         String
  email        String  @unique
  password     String
  sessions     Session[]
}

model Session {
  id           Int @id @default(autoincrement()) 
  creationDate DateTime @default(now())
  updatedDate  DateTime?
  valid        Boolean @default(true)
  token        String @unique
  adm          Adm @relation(fields: [admId], references: [id])
  admId        Int
}